# Run this SQL query to get port and address:
# SELECT 
#     DISTINCT M_DATABASE.HOST as "HANA_ADDRESS",
#     M_SERVICES.SQL_PORT as "HANA_PORT"
# FROM M_DATABASE, M_SERVICES
# WHERE M_SERVICES.SERVICE_NAME = 'indexserver'
#   AND M_SERVICES.COORDINATOR_TYPE = 'MASTER';
# ======================================================== #

HANA_USER=your_username
HANA_PASSWORD=your_password
HANA_ADDRESS=your_server_address
HANA_PORT=your_port_number